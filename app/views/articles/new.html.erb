<div class="d-flex justify-content-between m-4">
  <%= link_to "wikinimous", articles_path %>
  <%= link_to "New Article", articles_path %>
</div>
<h2 class= "mt-4">New Article</h2>
<%= form_with model: @article do |f| %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :content %>
<%= f.text_field :content %>
<%= f.submit %>
<% end %>

<%= link_to "Back", articles_path %>
